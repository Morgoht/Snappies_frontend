{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run ng -- serve --proxy-config proxy-backend.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test:unit": "ng test",
    "serve:ssr:front": "node dist/front/server/server.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
